<h3 class="text-danger">
    <%= messages?.error?? '' %>
</h3>
<div>
    <form action="/login" method="post">
        <div class="mb-2">
            <input type='text' name="username" required />
        </div>
        <div class="mb-2">
            <input type='password' name="password" required />
        </div>
        <button type="submit">
            signin
        </button>
        <a href="/register">signup  </a>
    </form>
</div>